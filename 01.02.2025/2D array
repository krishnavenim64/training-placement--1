#include <stdio.h>

int main() {

int rows, columns, sum = 0;

printf("Enter rows: "); scanf("%d", &rows); printf("Enter columns: "); scanf("%d", &columns); int array[rows] [columns]; printf("Enter elements: "); for (int i = 0; i < rows; i++) {

for (int j = 0; j < columns; j++) {

scanf("%d", &array[i][j]);

sum += array[i][j]; // Update sum with current element

}

}

printf("Sum: %d\n", sum);

return 0;

}
