#include <stdio.h>

int factorial(int n) {

int result = 1;

for (int i = 1; i <= n; i++) {

result = i; // Multiply result by i

}

return result;

}

int main() {

int number;

printf("Input: ");

scanf("%d", &number);

printf("The factorial of %d is: %d\n", number, factorial(number));

return 0;

