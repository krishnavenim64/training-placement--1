

#include <stdio.h>

int main() {

int size, sum = 0;

printf("Enter the size of the array: ");

scanf("%d", &size);

int array[size];

printf("Enter elements: ");

for (int i = 0; i < size; i++) {

scanf("%d", &array[i]);

sum += array[i]; // Update sum with current element

}

printf("Sum of elements: %d\n", sum);

return 0;

}
